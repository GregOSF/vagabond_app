<h1>New Story</h1>
<%= form_for @story do |f| %>
  <%= f.text_field :author, placeholder: "Author" %><br>
  <%= f.text_field :title, placeholder: "Title" %><br>
  <%= f.text_area :body, placeholder: "Body" %><br>
  <%= f.select :city_id, [["San Francisco", 2]] %><br>
  <%= f.submit "Save Story" %>
<% end %>
